# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

#source 'https://github.com/CocoaPods/Specs.git'
target 'RCRTCQuickDemo' do
# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

# Pods for RCRTCQuickDemo
pod 'RongCloudRTC/RongRTCLib', '~> 5.2.4'
pod 'RongCloudRTC/RongFaceBeautifier', '~> 5.2.4'  #美颜（可选）
pod 'RongCloudRTC/RongRTCPlayer', '~> 5.2.4'  #融云 CDN + 混音网络资源文件（可选）
pod 'RongCloudRTC/RongVoiceBeautifier', '~> 5.2.4'   #美声特效（可选）
pod 'RongCloudRTC/RongCallKit', '~> 5.2.4' #音视频通话
end

target 'ScreenShare' do
# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

# Pods for RCRTCQuickDemo
pod 'RongCloudRTC/RongRTCReplayKitExt','~> 5.2.4' #屏幕共享 库

end
